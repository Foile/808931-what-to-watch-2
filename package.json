{
  "name": "what-to-watch",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Что посмотреть» от HTML Academy",
  "jest": {
    "moduleNameMapper": {
      "@history": "<rootDir>/src/history.js",
      "@helpers":  "<rootDir>/src/helpers/helpers.js",
      "@constants": "<rootDir>/src/const.js",
      "@selectors": "<rootDir>/src/selectors/selectors.js",
      "@pages(.*)$": "<rootDir>/src/components/pages$1",
      "@reducers(.*)$": "<rootDir>/src/reducers$1",
      "@components(.*)$": "<rootDir>/src/components$1",
      "@hocs(.*)$": "<rootDir>/src/hocs$1",
      "@src(.*)$": "<rootDir>/src$1"
    }
  },
  "scripts": {
    "eslint": "eslint --ext .jsx --ext .js src/",
    "test": "npm run eslint && jest",
    "build": "webpack --mode production",
    "start": "webpack-dev-server"
  },
  "devDependencies": {
    "^babel/core": "7.6.4",
    "^babel/preset-env": "7.6.3",
    "^babel/preset-react": "7.6.3",
    "axios-mock-adapter": "1.17.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint": "5.13.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-react": "7.16.0",
    "jest": "24.9.0",
    "react-redux-hoc-loader": "1.1.1",
    "react-test-renderer": "16.10.2",
    "redux-mock-store": "1.5.4",
    "webpack": "4.41.1",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "axios": "0.19.0",
    "moment": "2.24.0",
    "moment-duration-format": "2.3.2",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "recompose": "0.30.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  }
}
